# Database Configuration
DATABASE_URL=postgresql://user:password@localhost/ap_platform

# OCR Configuration (DeepSeek-OCR API)
DEEPSEEK_OCR_API_URL=https://api.deepseek.com/ocr
DEEPSEEK_OCR_API_KEY=your_api_key_here
OCR_TIMEOUT_SECONDS=30
OCR_MAX_RETRIES=3

# Storage Configuration (S3-compatible)
STORAGE_ENDPOINT_URL=https://s3.amazonaws.com
STORAGE_ACCESS_KEY_ID=your_access_key
STORAGE_SECRET_ACCESS_KEY=your_secret_key
STORAGE_BUCKET_NAME=ap-invoices
STORAGE_REGION=us-east-1

# Matching Configuration
MATCHING_TOLERANCE=0.01
